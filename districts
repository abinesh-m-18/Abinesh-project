        };
        const districtList = document.getElementById('districtList');
        const vegetableList = document.getElementById('vegetableList');

        Object.keys(districts).forEach(district => {
            const li = document.createElement('li');
            li.textContent = district;
            li.onclick = () => displayVegetables(district);
            districtList.appendChild(li);
        });

        function displayVegetables(district) {
            vegetableList.innerHTML = '';
            districts[district].forEach(veg => {
                const li = document.createElement('li');
                li.textContent = `${veg.name} - ₹${veg.price}`;
                vegetableList.appendChild(li);
            });
        }
    </script>
</body>
</html>
